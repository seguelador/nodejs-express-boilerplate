{
  "name": "nodejs-express-boilerplate",
  "version": "1.0.0",
  "description": "NodeJS + ExpressJS boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test jest --config jest.config.js --coverage",
    "start": "NODE_ENV=production node ./index.js --inspect",
    "dev": "NODE_ENV=development nodemon ./index.js",
    "debug": "NODE_ENV=development nodemon --inspect --inspect-brk ./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seguelador/nodejs-express-boilerplate"
  },
  "author": "",
  "license": "private",
  "bugs": {
    "url": "https://github.com/seguelador/nodejs-express-boilerplate/issues"
  },
  "homepage": "https://github.com/seguelador/nodejs-express-boilerplate",
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.0.1"
  }
}
